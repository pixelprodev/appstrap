{
  "name": "@pixelprodotco/appstrap",
  "version": "0.4.0",
  "description": "Uber-configurable mock server for bootstrapping single page apps",
  "main": "./src/Appstrap.js",
  "bin": "./src/cli.js",
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/pixelprodotco/appstrap",
  "scripts": {
    "deploy": "npm run lint && npm test",
    "deploy:ui": "webpack --config webpack/interface.webpack.config.js --mode production",
    "deploy:core": "webpack --config webpack/core.webpack.config.js --mode production",
    "dev:ui": "webpack --config webpack/interface.webpack.config.js --mode development --watch --progress --colors",
    "dev:core": "webpack --config webpack/core.webpack.config.js --mode development --watch --progress --colors",
    "lint": "standard",
    "test": "jest",
    "test-config-generate": "node dist/cli.js init",
    "test-config": "node dist/cli.js start -c ./_test/_testConfig/config.js --preserve false"
  },
  "dependencies": {
    "async-retry": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "commander": "^2.14.1",
    "decache": "^4.4.0",
    "detect-port": "^1.2.3",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "lodash.merge": "^4.6.1",
    "no-cache-loader": "^1.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "require-all": "^2.2.0",
    "sleep-promise": "2.0.0",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@storybook/react": "^4.0.0-alpha.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.13.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.5.0",
    "react": "^16.4.0",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "shebang-loader": "0.0.1",
    "sinon": "^5.0.10",
    "standard": "^11.0.1",
    "supertest": "^3.0.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^2.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "standard": {
    "envs": [
      "jest",
      "browser"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "moduleNameMapper": {
      "webpack-dynamic-require": "<rootDir>/webpack-dynamic-require.js"
    }
  }
}
